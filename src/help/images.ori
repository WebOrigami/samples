// Return only the image files in the given tree
(tree) => Tree.mask(tree, Tree.globKeys({
  // Apply these globs at all levels
  "**": {
    "*.gif": true
    "*.ico": true
    "*.jpeg": true
    "*.jpg": true
    "*.png": true
    "*.svg": true
    "*.tif": true
    "*.tiff": true
    "*.webp": true
  }
}))
